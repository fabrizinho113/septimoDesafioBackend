<div class="m-5">
    {{#if products}}
    <h2 class="text-success">Products</h2>
        <table class="table table-bordered">
            <thead class="table-dark">
                <tr>
                <th scope="col">Title</th>
                <th scope="col">Price</th>
                <th scope="col">Thumbnail</th>
                </tr>
            </thead>
            <tbody>
                {{#each products}}
                <tr>
                    <td>{{this.title}}</td>
                    <td>{{this.price}}</td>
                    <td><img class="logo" src={{this.thumbnail}}/></td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    {{else}}
        <div>
            <p class="empty-list">No hay productos</p>
        </div>
    {{/if}}
</div>